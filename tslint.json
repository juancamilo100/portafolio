{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ],
        "indent": [true, "tabs", 4],
        "object-literal-sort-keys": false,
        "no-console": {
            "severity": "none"
        },
        "variable-name": false
    },
    "rulesDirectory": []
}